{
	"name": "ligas",
	"version": "2.0.0",
	"private": "true",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"format": "prettier --write --ignore-unknown --plugin-search-dir . .",
		"format:check": "prettier --check --ignore-unknown --plugin-search-dir . .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:verbose": "vitest --reporter verbose",
		"gen:assets": "npx pwa-asset-generator ./assets/logo.html ./public/icons -i index.html -m ./src/manifest.json --path-override /icons --splash-only && npx pwa-asset-generator ./assets/logo.html ./public/icons -i index.html -m ./src/manifest.json --path-override /icons -d && prettier -w ./src/manifest.json ./index.html"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.4.2",
		"@tsconfig/svelte": "^5.0.0",
		"autoprefixer": "^10.4.14",
		"daisyui": "^2.52.0",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.4",
		"svelte-feather-icons": "^4.0.1",
		"svelte-frappe-charts": "^1.10.0",
		"svelte-i18n": "^3.6.0",
		"svelte-preprocess": "^5.0.4",
		"svelte-spa-router": "^3.3.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.3.9",
		"vite-plugin-pwa": "^0.16.4",
		"vitest": "^0.29.8",
		"workbox-window": "^7.0.0"
	}
}
