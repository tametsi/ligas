{
	"name": "ligas",
	"version": "2.0.0",
	"private": "true",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"format": "prettier --write --ignore-unknown --plugin prettier-plugin-svelte .",
		"format:check": "prettier --check --ignore-unknown --plugin prettier-plugin-svelte .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:verbose": "vitest --reporter verbose",
		"gen:assets": "npx pwa-asset-generator ./assets/logo.html ./public/icons -i index.html -m ./src/manifest.json --path-override /icons --splash-only && npx pwa-asset-generator ./assets/logo.html ./public/icons -i index.html -m ./src/manifest.json --path-override /icons -d && prettier -w ./src/manifest.json ./index.html"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.4.6",
		"@tsconfig/svelte": "^5.0.2",
		"autoprefixer": "^10.4.16",
		"daisyui": "^3.9.3",
		"jsdom": "^22.1.0",
		"lucide-svelte": "^0.261.0",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^3.59.2",
		"svelte-check": "^3.5.2",
		"svelte-frappe-charts": "^1.10.0",
		"svelte-i18n": "^3.7.4",
		"svelte-preprocess": "^5.0.4",
		"svelte-spa-router": "^3.3.0",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vite-plugin-pwa": "^0.16.5",
		"vitest": "^0.34.6",
		"workbox-window": "^7.0.0"
	}
}
