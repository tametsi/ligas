{
	"name": "ligas",
	"version": "1.2.0",
	"private": "true",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"format": "prettier --write --ignore-unknown --plugin-search-dir . .",
		"format:check": "prettier --check --ignore-unknown --plugin-search-dir . .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:verbose": "vitest --reporter verbose",
		"gen:assets": "npx pwa-asset-generator ./assets/logo.html ./public/icons -i index.html -m ./src/manifest.json --path-override /icons --splash-only && npx pwa-asset-generator ./assets/logo.html ./public/icons -i index.html -m ./src/manifest.json --path-override /icons -d && prettier -w ./src/manifest.json ./index.html"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^1.0.1",
		"@tsconfig/svelte": "^2.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"sass": "^1.49.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.7",
		"svelte-feather-icons": "^4.0.0",
		"svelte-frappe-charts": "^1.9.2",
		"svelte-preprocess": "^4.9.8",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4",
		"vite": "^3.1.4",
		"vite-plugin-pwa": "^0.13.1",
		"vitest": "^0.22.1",
		"workbox-window": "^6.5.4"
	}
}
